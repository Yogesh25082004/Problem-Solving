#include <stdio.h>

int main()
{

    int x;
    int y;
    int z;
    printf("Enter the first number:");
    scanf("\n %d", &x);
    printf("Enter the second number:");
    scanf("\n %d", &y);
    printf("Enter the third number:");
    scanf("\n %d", &z);

    if (x > y && x > z)
    {
        printf("First number is greater");
    }
    if (y > x && y > z)
    {
        printf("Second number is greater");
    }
    else
    {
        printf("Third number is greater");
    }
    return 0;
}
